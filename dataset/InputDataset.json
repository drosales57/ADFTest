{
	"name": "InputDataset",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureStorageLinkedService",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"temp": {
				"type": "BadstringType",
				"defaultValue": "input"
			}
		},
		"type": "AzureBlob",
		"typeProperties": {
			"format": {
				"type": "TextFormat",
				"columnDelimiter": ",",
				"rowDelimiter": "",
				"nullValue": "\\N",
				"treatEmptyAsNull": true,
				"skipLineCount": 0,
				"firstRowAsHeader": false
			},
			"fileName": "empty.txt",
			"folderPath": {
				"value": "testblobstorage/@{dataset().temp}",
				"type": "Expression"
			}
		}
	},
	"type": "Microsoft.DataFactory/factories/datasets"
}
