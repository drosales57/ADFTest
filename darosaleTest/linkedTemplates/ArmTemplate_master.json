{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "darosaleTest"
		},
		"integrationRuntime_integrationRuntime2": {
			"type": "string",
			"metadata": "Integration runtime name",
			"defaultValue": "integrationRuntime2"
		},
		"AzureDatabricks1_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricks1'"
		},
		"AzureStorageLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureStorageLinkedService'"
		},
		"AzureDatabricks1_properties_typeProperties_newClusterNodeType": {
			"type": "string",
			"defaultValue": "vvv"
		},
		"AzureDatabricks1_properties_typeProperties_newClusterNumOfWorker": {
			"type": "string",
			"defaultValue": "1"
		},
		"AzureDatabricks1_properties_typeProperties_newClusterVersion": {
			"type": "string",
			"defaultValue": "ddd"
		},
		"AzureDatabricks1_properties_connectVia_referenceName": {
			"type": "string"
		},
		"AzureDatabricks2_properties_typeProperties_accessToken_secretName": {
			"type": "string",
			"defaultValue": "@linkedService().secretName"
		},
		"AzureDatabricks2_properties_typeProperties_existingClusterId": {
			"type": "string"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string"
		},
		"RestService1_properties_typeProperties_url": {
			"type": "string"
		},
		"integrationRuntime2_properties_type": {
			"type": "string",
			"defaultValue": "Managed"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"integrationRuntime_integrationRuntime2": {
						"value": "[parameters('integrationRuntime_integrationRuntime2')]"
					},
					"AzureStorageLinkedService_connectionString": {
						"value": "[parameters('AzureStorageLinkedService_connectionString')]"
					},
					"AzureDatabricks1_accessToken": {
						"value": "[parameters('AzureDatabricks1_accessToken')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"RestService1_properties_typeProperties_url": {
						"value": "[parameters('RestService1_properties_typeProperties_url')]"
					},
					"integrationRuntime2_properties_type": {
						"value": "[parameters('integrationRuntime2_properties_type')]"
					},
					"AzureDatabricks1_properties_typeProperties_newClusterNodeType": {
						"value": "[parameters('AzureDatabricks1_properties_typeProperties_newClusterNodeType')]"
					},
					"AzureDatabricks1_properties_typeProperties_newClusterNumOfWorker": {
						"value": "[parameters('AzureDatabricks1_properties_typeProperties_newClusterNumOfWorker')]"
					},
					"AzureDatabricks1_properties_typeProperties_newClusterVersion": {
						"value": "[parameters('AzureDatabricks1_properties_typeProperties_newClusterVersion')]"
					},
					"AzureDatabricks1_properties_connectVia_referenceName": {
						"value": "[parameters('AzureDatabricks1_properties_connectVia_referenceName')]"
					},
					"AzureDatabricks2_properties_typeProperties_accessToken_secretName": {
						"value": "[parameters('AzureDatabricks2_properties_typeProperties_accessToken_secretName')]"
					},
					"AzureDatabricks2_properties_typeProperties_existingClusterId": {
						"value": "[parameters('AzureDatabricks2_properties_typeProperties_existingClusterId')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ArmTemplate_0')]"
			]
		},
		{
			"name": "ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ArmTemplate_1')]"
			]
		}
	]
}